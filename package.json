{
	"name": "laravel-extra-intellisense",
	"displayName": "Laravel Extra Intellisense",
	"description": "better intellisense for laravel projects.",
	"version": "0.1.5",
	"publisher": "amiralizadeh9480",
	"icon": "images/icon.png",
	"engines": {
		"vscode": "^1.30.0"
	},
	"categories": [
		"Languages"
	],
	"activationEvents": [
		"onLanguage:php",
		"onLanguage:blade"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/amir9480/vscode-laravel-extra-intellisense"
	},
	"main": "./out/extension.js",
	"contributes": {},
	"scripts": {
		"vscode:prepublish": "webpack --mode production",
		"compile": "webpack --mode none",
		"watch": "webpack --mode none --watch",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test",
		"test-compile": "tsc -p ./"
	},
	"dependencies": {
		"php-parser": "^3.0.0-prerelease.8"
	},
	"devDependencies": {
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.0.8",
		"ts-loader": "^6.0.3",
		"tslint": "^5.17.0",
		"typescript": "^3.5.2",
		"vscode": "^1.1.34",
		"webpack": "^4.34.0",
		"webpack-cli": "^3.3.4"
	}
}
