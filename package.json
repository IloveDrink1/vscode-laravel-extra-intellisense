{
	"name": "laravel-extra-intellisense",
	"displayName": "Laravel Extra Intellisense",
	"description": "better intellisense for laravel projects.",
	"version": "0.1.2",
	"publisher": "amiralizadeh9480",
	"icon": "images/icon.png",
	"engines": {
		"vscode": "^1.30.0"
	},
	"categories": [
		"Languages"
	],
	"activationEvents": [
		"onLanguage:php",
		"onLanguage:blade"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/amir9480/vscode-laravel-extra-intellisense"
	},
	"main": "./out/extension.js",
	"contributes": {},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.1.4",
		"vscode": "^1.1.25",
		"tslint": "^5.8.0",
		"@types/node": "^8.10.25",
		"@types/mocha": "^2.2.42",
		"php-parser": "^3.0.0-prerelease.8"
	}
}
